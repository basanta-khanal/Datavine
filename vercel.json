{
  "version": 2,
  "name": "datavine-ai",
  "env": {
    "NEXT_PUBLIC_API_URL": "https://your-railway-app-name.railway.app",
    "NEXT_PUBLIC_APP_NAME": "DataVine.ai",
    "NEXT_PUBLIC_APP_VERSION": "1.0.0"
  },
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "Referrer-Policy",
          "value": "strict-origin-when-cross-origin"
        }
      ]
    }
  ]
} 